; Purely decorative modules

; template arrow thing
[module/<--]
; color to color
type = custom/text
content = "%{T2} %{T-}"
content-font = 5
content-foreground = ${colors.background-alt}
content-background = ${colors.background}

[module/<---]
inherit = module/<--
content-foreground = ${colors.background}
content-background = ${colors.background-alt}

[module/-->]
type = custom/text
content = "%{T2} %{T-}"
content-font = 5
content-foreground = ${colors.background-alt}
content-background = ${colors.background}

[module/--->]
inherit = module/-->
content-foreground = ${colors.background}
content-background = ${colors.background-alt}

[module/_]
type = custom/text
content = " "

; vim:ft=dosini
